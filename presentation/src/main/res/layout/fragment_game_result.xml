<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.match.game_result.GameResultFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_members_rank"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginVertical="9dp"
        android:layout_marginStart="9dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/rv_member_match_rank_item" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_members"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginVertical="9dp"
        android:layout_marginEnd="9dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rv_members_rank"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/rv_member_match_item" />


</androidx.constraintlayout.widget.ConstraintLayout>
